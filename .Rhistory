avg_r_mo = c()
for (m in 1:length(mos)){
mo_repros
for (y in 1:length(years)){
tmp = subset(femaledata, yr == years[y])
tmp = subset(tmp, mo == mos[m])
if (nrow(tmp) > 0){
num_females = nrow(tmp)
repro = subset(tmp, nipples == "E" | nipples == "B" | pregnant == "P")
